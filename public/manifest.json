{
  "manifest_version": 2,
  "name": "Eye blinker",
  "author": "Muhsin.K",
  "version": "1.0",
  "description": "Chrome extension that will helps to follow 20-20-20 rule",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup",
    "default_icon": "icon.png"
  },
  "permissions": ["notifications", "storage"],
  "background": {
    "scripts": ["app/background.js"],
    "persistent": false
  }
}
